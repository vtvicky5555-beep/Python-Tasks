class Book:
    def __init__(self,title,author,price):
        self.title=title
        self.author=author
        self.price=price
    #print book detail
    def display(self):
        print(f"Title : {self.title},Author : {self.author} Price ={self.price}")
book_1=Book("Wings fire","Apj",250)
book_2=Book("thirukural","thiruvalluvar",890)
book_1.display()
book_2.display()
